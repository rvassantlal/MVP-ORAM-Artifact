#######################
# Required parameters #
#######################
#IP address where the controller will listen for workers' connections
controller.listening.ip = 127.0.0.1

#Port number where the controller will listen for workers' connections
controller.listening.port = 12000

#Number of machines
global.worker.machines = 6

#Controller benchmark strategy class implementing IBenchmarkStrategy
controller.benchmark.strategy = benchmark.MeasurementBenchmarkStrategy

#Worker setup class implementing ISetupWorker
controller.worker.setup = benchmark.MeasurementSetup

#Worker event processor class implementing IWorkerEventProcessor
controller.worker.processor = benchmark.MeasurementEventProcessor

#############################################################################
# Extra parameters provided in IBenchmarkStrategy through Properties object #
#############################################################################
# Path to a directory. All results will be saved as subfolders within this path. E.g., /home/<user>/
output.path = <path to a directory>

# Measure resource usage (CPU, memory, and network) (true or false)
measure_resources = false

# Space separated list of fault thresholds (>= 1). E.g. "1 2"
fault_thresholds = 1 2

#Space separated list of servers' IP addresses
server_ips = 127.0.0.1 127.0.0.1 127.0.0.1

# ORAM parameters
bucket_sizes = 4
block_sizes = 8
write_back_threshold = 1
zipf_parameters = 1.0

# ORAM storage size in megabytes
storage_size = 1

#Test length in seconds
measurement_duration = 120
clients_per_round = 1 5 10 15 25 40 50
